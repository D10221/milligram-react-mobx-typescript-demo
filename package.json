{
  "name": "milligramo",
  "displayName": "Milligram Demo",
  "version": "1.0.0",
  "description": "Milligram, React, Typescript, Electron ...demo",
  "main": "built/main.js",
  "icon": "resources/icon-128x128.png",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/D10221/milligram-react-mobx-typescript-demo"
  },
  "scripts": {
    "postinstall": "goofoffline outDir=\"src/fonts\" \"https://fonts.googleapis.com/css?family=Roboto\"",
    "link": "npm link electron",
    "test": "mocha test.js",
    "build": "npm run build:main && npm run bundle",
    "build:main": "tsc -p . ",
    "build:main+bundle:client": "tsc -p . && npm run bundle:client",
    "format": "tsfmt -r",
    "clean": "rimraf ./built",
    "rebuild": "npm run clean && npm run build && npm run bundle",
    "start": "electron .",
    "bundle:vendor": "webpack --config webpack/vendor.webpack.config.js",
    "bundle:client": "webpack --config webpack/webpack.config.js",
    "bundle": "npm run bundle:vendor && npm run bundle:client",
    "re:build": "npm run clean && npm run build",
    "png-to-icns": "nicns --in esources/icon.png --out icon.icns"
  },
  "keywords": [
    "miligram",
    "react",
    "mobx",
    "electron",
    "typescript"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/debug": "0.0.29",
    "@types/electron": "^1.4.37",
    "@types/electron-json-storage": "0.0.19",
    "@types/mocha": "^2.2.41",
    "@types/react": "^15.0.24",
    "@types/react-dom": "^15.5.0",
    "@types/react-router": "^4.0.9",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.1",
    "electron": "^1.6.8",
    "electron-builder": "^17.8.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "mocha": "^3.3.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.17.0",
    "ts-loader": "^2.0.3",
    "tslint": "^5.2.0",
    "typescript": "^2.3.2",
    "typescript-formatter": "^5.2.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "debug": "^2.6.6",
    "electron-json-storage": "^3.0.5",
    "milligram": "^1.3.0",
    "milligram-themes-clone": "git+https://github.com/D10221/milligram-themes.git#_dev_000_",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "mobx-react-router": "^4.0.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1"
  },
  "build": {
    "appId": "milligramo",
    "files": [
      "built",
      "node_modules",
      "resources",
      "main.js"
    ],
    "mac": {},
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  }
}
